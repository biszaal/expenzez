{
  "expo": {
    "name": "Expenzez",
    "slug": "expenzez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expenzez",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "execute-api.eu-west-2.amazonaws.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2",
              "NSExceptionRequiresForwardSecrecy": true
            },
            "cognito-idp.eu-west-2.amazonaws.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2",
              "NSExceptionRequiresForwardSecrecy": true
            },
            "auth.truelayer.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2",
              "NSExceptionRequiresForwardSecrecy": true
            }
          }
        },
        "NSCameraUsageDescription": "This app uses the camera to scan receipts for expense tracking.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to attach receipt images to expenses.",
        "NSFaceIDUsageDescription": "This app uses Face ID for secure authentication and app access.",
        "NSLocationWhenInUseUsageDescription": "This app uses location data to categorize expenses and find nearby merchants."
      },
      "bundleIdentifier": "com.biszaal.expenzez"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": false,
      "package": "com.biszaal.expenzez",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "organization": "expenzez",
          "project": "expenzez-mobile"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "849d0f63-bfaa-4d69-aa89-818904092d8b"
      },
      "sentryDsn": "https://your-sentry-dsn@sentry.io/project-id"
    },
    "owner": "biszaal",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/849d0f63-bfaa-4d69-aa89-818904092d8b"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}