{
  "expo": {
    "name": "Expenzez",
    "slug": "expenzez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expenzez",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Smart expense tracking with banking integration, AI insights, and comprehensive financial management for UK users.",
    "keywords": [
      "expense",
      "finance",
      "banking",
      "budget",
      "money",
      "AI"
    ],
    "privacy": "public",
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:expenzez.com",
        "applinks:www.expenzez.com"
      ],
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "execute-api.eu-west-2.amazonaws.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2",
              "NSExceptionRequiresForwardSecrecy": true
            },
            "cognito-idp.eu-west-2.amazonaws.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2",
              "NSExceptionRequiresForwardSecrecy": true
            },
            "auth.truelayer.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2",
              "NSExceptionRequiresForwardSecrecy": true
            }
          }
        },
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "expenzez-redirect",
            "CFBundleURLSchemes": ["expenzez"]
          }
        ],
        "NSCameraUsageDescription": "This app uses the camera to scan receipts for expense tracking.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to attach receipt images to expenses.",
        "NSFaceIDUsageDescription": "This app uses Face ID for secure authentication and app access.",
        "NSLocationWhenInUseUsageDescription": "This app uses location data to categorize expenses and find nearby merchants."
      },
      "bundleIdentifier": "com.biszaal.expenzez"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "package": "com.biszaal.expenzez"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "abdbfccb-cbc1-4be7-8fcc-76ac70f61747"
      }
    },
    "owner": "biszaal"
  }
}
